{
  "name": "ml-cat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "tsx watch server/api.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "npm run build:client && npm run build:server && npm run copy:data",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "copy:data": "node -e \"const fs=require('fs');const path=require('path');const src='src/data/groceryEmbeddings.json';const dest='dist/src/data/groceryEmbeddings.json';if(fs.existsSync(src)){fs.mkdirSync(path.dirname(dest),{recursive:true});fs.copyFileSync(src,dest);console.log('Copied embeddings file');}else{console.warn('Embeddings file not found');}\"",
    "start": "node dist/server/api.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "precompute": "tsx scripts/precomputeEmbeddings.ts"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.4"
  }
}

